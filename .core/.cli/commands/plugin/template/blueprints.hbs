export default [
    {{#each blueprints}}
    {
        ID: '{{this.ID}}',
        description: '{{this.description}}',
        className: '{{this.className}}',
        {{#if this.admin}}
        sections: {
            sidebar: {
                zones: ['admin-sidebar'],
                meta: {},
            },
            main: {
                zones: [
                    'admin-header',
                    'admin-{{this.zone}}',
                    'admin-actions'
                ],
                meta: {},
            },
            tools: {
                zones: ['admin-tools'],
            },
        },
        meta: {
            admin: true,
            builtIn: {{this.builtIn}},
            namespace: 'admin-page'
        },
        {{else}}
        sections: {
            sidebar: {
                zones: ['sidebar'],
                meta: {},
            },
            main: {
                zones: [
                    'header',
                    '{{this.zone}}',
                    'actions',
                    'footer',
                ],
                meta: {},
            },
        },
        meta: {
            admin: false,
            builtIn: {{this.builtIn}},
            namespace: 'page'
        },
        {{/if}}
    },
    {{/each}}
];
