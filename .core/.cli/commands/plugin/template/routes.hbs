module.exports = [
    {{#each routes}}
    {
      blueprint: '{{this.blueprint}}',
      route: '{{this.route}}',
      meta: {
          builtIn: {{this.builtIn}},
          {{#if this.admin}}
          app: 'admin',
          {{/if}}
      },
      capabilities: {{{this.capabilities}}},
    },
    {{/each}}
];
