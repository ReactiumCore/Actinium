        "chalk": "^2.4.2",
        "cookie-parser": "*",
        "cookie-session": "^1.4.0",
        "cors": "^2.8.5",
        "cross-env": "^7.0.2",
        "ejs": "*",
        "events": "^3.2.0",
        "express": "*",
        "fast-deep-equal": "^3.1.3",
        "file-api": "^0.10.4",
        "fs-extra": "^9.0.1",
        "globby": "^11.0.1",
        "handlebars": "^4.7.6",
        "https": "^1.0.0",
        "js-beautify": "^1.11.0",
        "js-cookie": "^2.2.1",
