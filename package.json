        "lint-staged": "^9.4.2",
        "nodemon": "^2.0.4",
        "prettier": "^1.18.2"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Atomic-Reactor/Actinium-2.0"
    },
    "engines": {
        "node": "12.13.0",
        "npm": ">=6.12.0"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,json,css,md}": [
            "prettier --write",
            "git add"
        ]
    }
}
